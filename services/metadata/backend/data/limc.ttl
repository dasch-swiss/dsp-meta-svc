            dsp:hasDate "2021-09-02"^^xsd:date ;
            dsp:hasURL [ a sdo:URL ;
                    sdo:propertyID [ a sdo:PropertyValue ;
                            sdo:propertyID "CC BY-NC 4.0" ] ;
                    sdo:url "https://creativecommons.org/licenses/by-nc/4.0/" ] ] ;
    dsp:hasQualifiedAttribution [ a prov:Attribution ;
            dsp:hasRole "publisher"^^xsd:string ;
            prov:agent dsp:dsp-080E-organization-005 ] ;
    dsp:hasStatus "Ongoing"^^xsd:string ;
    dsp:hasTitle "Dataset of Digital Lexicon Iconographicum Mythologiae Classicae (Digital LIMC)"^^xsd:string ;
    dsp:hasTypeOfData "Image"^^xsd:string,
        "Text"^^xsd:string .

