    dsp:hasHowToCite "HdM-Bern"^^xsd:string ;
    dsp:hasKeyword "Bern"@de,
        "19 Century"@en,
        "Concert"@en,
        "Music"@en,
        "Musicology"@en,
        "Opera"@en,
        "Spectales"@en,
        "Switzerland"@en,
        "Theater history"@en,
        "Theatre"@en ;
