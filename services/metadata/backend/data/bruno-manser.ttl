dsp:dsp-0813-grant-001 a dsp:Grant ;
    dsp:hasFunder dsp:dsp-0813-organization-001 .

dsp:dsp-0813-project a dsp:Project ;
    dsp:hasAlternativeName "Annotating Bruno Manser's Diaries"@en ;
    dsp:hasDataset dsp:dsp-0813-dataset-001 ;
    dsp:hasDescription "Bruno Manser has left a variety of media in which he documented the nomadic life of the Penan. The project aims to show and annotate the everyday life moments of a nomadic lifestyle across media as diary entries, video footage and photographs of Bruno Manser."@en ;
    dsp:hasDiscipline [ a sdo:URL ;
            sdo:propertyID [ a sdo:PropertyValue ;
                    sdo:propertyID "Cultural anthropology" ] ;
            sdo:url "http://skos.um.es/unesco6/5101" ],
        [ a sdo:URL ;
            sdo:propertyID [ a sdo:PropertyValue ;
                    sdo:propertyID "Social anthropology" ] ;
            sdo:url "http://skos.um.es/unesco6/5103" ] ;
    dsp:hasFunder dsp:dsp-0813-organization-001 ;
    dsp:hasHowToCite "Bruno Manser/Bruno Manser Fonds"^^xsd:string ;
    dsp:hasKeyword "Bruno Manser Fonds"@de,
        "Nomaden"@de,
        "Regenwald"@de,
        "Waldnomaden"@de,
        "Bruno Manser"@en,
        "Hunters and Gatherers"@en,
        "Nomads"@en,
        "Penan"@en,
        "Rainforest"@en,
        "nomadic lifestyle"@en ;
    dsp:hasName "Annotating Media of Bruno Manser"^^xsd:string ;
    dsp:hasShortcode "0813"^^xsd:string ;
    dsp:hasSpatialCoverage [ a sdo:URL ;
            sdo:propertyID [ a sdo:PropertyValue ;
                    sdo:propertyID "Sarawak" ] ;
            sdo:url "https://www.geonames.org/1733038/sarawak.html" ],
        [ a sdo:URL ;
            sdo:propertyID [ a sdo:PropertyValue ;
                    sdo:propertyID "Switzerland" ] ;
            sdo:url "https://www.geonames.org/2658434/switzerland.html" ] ;
    dsp:hasStartDate "2014-05-01"^^xsd:date ;
    dsp:hasTeaser "XX - new property"^^xsd:string ;
    dsp:hasTemporalCoverage "1984 - 2000"@de,
        "1984 - 2000"@en,
        "1984 - 2000"@fr ;
    dsp:hasURL [ a sdo:URL ;
            sdo:url "https://data.dasch.swiss/bmf/" ] .

