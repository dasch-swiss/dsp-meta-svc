                    sdo:url "https://creativecommons.org/licenses/by-nc-sa/4.0/" ] ] ;
    dsp:hasQualifiedAttribution [ a prov:Attribution ;
            dsp:hasRole "Author"^^xsd:string ;
            prov:agent dsp:dsp-0813-person-002 ],
        [ a prov:Attribution ;
            dsp:hasRole "Projektleiter"^^xsd:string ;
            prov:agent dsp:dsp-0813-person-001 ],
        [ a prov:Attribution ;
            dsp:hasRole "Editor"^^xsd:string ;
            prov:agent dsp:dsp-0813-organization-001 ] ;
    dsp:hasStatus "Ongoing"^^xsd:string ;
